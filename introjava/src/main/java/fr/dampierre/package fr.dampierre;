package fr.dampierre;

public class Suite2 {
    public static void main(String[] args) {
        int nombreA = 0;
        int nombreB = 1;
        System.out.println(nombreA);

    for (int i = 0; i<20; i++){
        System.out.print(nombreB);
        nombreB = nombreB+nombreA;
        nombreA = nombreB;
        }

    }

        

